<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nanosoft.sreda.Fragement.TechNamesReportFragment">

    <Spinner
        android:id="@+id/techWiseSpinner"
        android:layout_width="@dimen/_113sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_margin="@dimen/_10sdp"
        android:entries="@array/technology_name_arrays"
        android:prompt="@string/technology_name_prompt">

    </Spinner>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_50sdp"
        android:orientation="horizontal">

        <include layout="@layout/header_technology_wise" />

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerviewTechWise"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </android.support.v7.widget.RecyclerView>

    </LinearLayout>


</FrameLayout>
